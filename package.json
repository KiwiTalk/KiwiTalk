{
  "name": "kiwi-talk",
  "version": "0.0.1",
  "private": true,
  "description": "open kiwi talk",
  "authors": [
    "mocha",
    "JellyBrick",
    "Scripter36",
    "Su-Yong",
    "nnnlog",
    "sangwon090",
    "storycraft"
  ],
  "main": "entry.js",
  "node-remote": "*://localhost",
  "window": {
    "toolbar": false,
    "icon": "img/android-chrome-512x512.png"
  },
  "scripts": {
    "start:nw": "node_modules/.bin/nwbuild -r . --dev",
    "start:webpack": "node_modules/.bin/webpack serve --mode=development",
    "start": "node_modules/.bin/concurrently \"npm:start:webpack\" \"npm:start:nw\"",
    "build": "node_modules/.bin/webpack --mode=production",
    "release": "npm run build && node_modules/.bin/concurrently \"npm:package:win64\" \"npm:package:mac64\" \"npm:package:linux64\"",
    "package:win64": "node buildscript.js win64",
    "package:mac64": "node buildscript.js osx64",
    "package:linux64": "node buildscript.js linux64"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "bson": "^4.0.4",
    "image-size": "^0.9.1",
    "localforage": "^1.7.3",
    "node-kakao": "^3.1.7",
    "open-graph-scraper": "^4.7.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.1",
    "react-spoiler": "^0.4.3",
    "styled-components": "^5.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/bson": "^4.0.2",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.20",
    "@types/nw.js": "^0.13.11",
    "@types/open-graph-scraper": "^4.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-google": "^0.14.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "nw-builder": "^3.5.7",
    "os-tmpdir": "^2.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.13",
    "wait-on": "^5.2.1",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  },
  "build": {
    "appId": "io.github.kiwitalk",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "public/img/android-chrome-512x512.png"
    }
  }
}
