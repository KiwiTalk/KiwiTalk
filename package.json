{
  "name": "kiwi-talk",
  "version": "0.1.0",
  "private": true,
  "description": "open kiwi talk",
  "authors": [
    "mocha"
  ],
  "main": "public/entry.js",
  "homepage": "./",
  "scripts": {
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test:react": "react-scripts test",
    "eject:react": "react-scripts eject",
    "start:electron": "electron .",
    "start": "concurrently \"cross-env BROWSER=none yarn start:react\" \"wait-on http://localhost:3000 && yarn start:electron",
    "build": "yarn build:react && electron-builder",
    "release": "yarn build:react && electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "electron-store": "^5.1.1",
    "is-electron": "^2.2.0",
    "node-kakao": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.1.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^7.0.3",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-is-dev": "^1.2.0",
    "wait-on": "^5.0.0"
  }
}
