{
  "name": "kiwi-talk",
  "version": "0.1.0",
  "private": true,
  "description": "open kiwi talk",
  "authors": [
    "mocha",
    "JellyBrick",
    "Scripter36",
    "SuYong",
    "nnnlog"
  ],
  "main": "public/entry.js",
  "window": {
    "frame": false,
    "toolbar": false
  },
  "homepage": "./",
  "scripts": {
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test:react": "react-scripts test",
    "eject:react": "react-scripts eject",
    "start:nw": "nw .",
    "start": "concurrently \"cross-env BROWSER=none yarn start:react\" \"wait-on http://localhost:3000 && yarn start:nw\"",
    "build": "yarn build:react && nwbuild .",
    "postinstall": "cd ./node_modules/node-kakao && ../.bin/tsc"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "localforage": "^1.7.3",
    "node-kakao": "github:storycraft/node-kakao#dev",
    "nwjs": "^1.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "request-promise": "^4.2.5",
    "styled-components": "^5.1.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.1.0",
    "@types/bson": "^4.0.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.0.1",
    "@types/node-forge": "^0.9.3",
    "@types/nw.gui": "^0.0.20",
    "@types/nw.js": "^0.13.9",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/request-promise": "^4.1.46",
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^7.0.3",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "nw": "^0.45.6",
    "nw-builder": "^3.5.7",
    "wait-on": "^5.0.0"
  },
  "build": {
    "appId": "io.github.kiwitalk",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "public/img/android-chrome-512x512.png"
    }
  }
}
